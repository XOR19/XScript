package xscript.lang;

public class Byte{

	private final byte value;

	public Byte(byte value){
		this.value = value;
	}
	
	public byte getValue(){
		return value;
	}
	
	@Override
	public String toString(){
		return Int.toString(value);
	}
	
	public static Byte valueOf(byte b){
		return new Byte(b);
	}
	
}
