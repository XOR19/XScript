package xscript.lang;

public class Int{

	private final int value;

	public Int(int value){
		this.value = value;
	}
	
	public int getValue(){
		return value;
	}
	
	public static native String toHex(int i);
	
	public static native String toHex(long l);

	public static Int valueOf(int i){
		return new Int(i);
	}
	
}
