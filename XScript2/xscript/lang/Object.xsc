package xscript.lang;

public class Object{
	
	public native Class getClass();
	
	public bool equals(Object other){
		return this==other;
	}
	
	public native long hashCode();
	
	public String toString(){
		return getClass().getName()+"@"+Long.toHexString(hashCode());
	}
	
	public native void notify();
	
	public native void notifyAll();
	
	public native void wait();
	
}
