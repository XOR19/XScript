package xscript.lang;

public class Short{

	private final short value;

	public Short(short value){
		this.value = value;
	}
	
	public short getValue(){
		return value;
	}
	
	@Override
	public String toString(){
		return Int.toString(value);
	}
	
	public static Short valueOf(short s){
		return new Short(s);
	}
	
}
